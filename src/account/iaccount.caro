
struct Call {
    to: ContractAddress;
    selector: felt252,
    calldata: Array<felt252>
}

#[starknet::interface]
trait IAccount {
    fn __execute__(calls: Array<Call>) -> Array<Span<felt252>>;
    fn __validate__(calls: Array<Call>) -> felt252;
    fn is_valid_signature(hash: felt252, signature: Array<felt252>) -> felt252;
}